<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>calServlet</servlet-name>
<!--  自己设计的Servlet Tomcat不识别所以会报红  -->
    <servlet-class>com.lilystu.tomcat.servlet.LilyCalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>calServlet</servlet-name>
    <url-pattern>/cal</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>cal2Servlet</servlet-name>
    <!--  自己设计的Servlet Tomcat不识别所以会报红  -->
    <servlet-class>com.lilystu.tomcat.servlet.LilyCal2Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cal2Servlet</servlet-name>
    <url-pattern>/cal2</url-pattern>
  </servlet-mapping>
</web-app>
